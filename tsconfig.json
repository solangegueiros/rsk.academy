{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@components": ["src/components"],
      "@configs/*": ["src/configs/*"],
      "@configs": ["src/configs"],
      "@constants/*": ["src/constants/*"],
      "@constants": ["src/constants"],
      "@context/*": ["src/context/*"],
      "@context": ["src/context"],
      "@contracts/*": ["src/contracts/*"],
      "@contracts": ["src/contracts"],
      "@hooks/*": ["src/hooks/*"],
      "@hooks": ["src/hooks"],
      "@layouts/*": ["src/layouts/*"],
      "@layouts": ["src/layouts"],
      "@lib/*": ["src/lib/*"],
      "@lib": ["src/lib"],
      "@store/*": ["src/store/*"],
      "@store": ["src/store"],
      "@theme/*": ["src/theme/*"],
      "@theme": ["src/theme"],
      "@utils/*": ["src/utils/*"],
      "@utils": ["src/utils"],
      "@i18n": ["./i18n"],
      "@test/*": ["test/*"],
      "@test": ["test"],
      "@type_chain": ["type_chain"],
      "@type_chain/*": ["type_chain/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules", "test/*", "cypress/*"]
}
